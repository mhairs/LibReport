                         "bcryptjs":  "^3.0.2",
                         "cors":  "^2.8.5",
                         "dotenv":  "^17.2.3",
                         "express":  "^5.1.0",
                         "express-rate-limit":  "^8.1.0",
                         "http-proxy-middleware":  "^3.0.3",
